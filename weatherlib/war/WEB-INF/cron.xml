<cronentries>
	<cron>
		<url>/cron/check</url>
		<description>check schedule every 5 minutes</description>
		<schedule>every 5 minutes</schedule>
	</cron>
	<cron>
		<url>/cron/check?recheck=true</url>
		<description>recheck schedule every 2 minutes</description>
		<schedule>every 2 minutes</schedule>
	</cron>
	<cron>
		<url>/cron/clearWeatherCache</url>
		<description>clear Weather Cache</description>
		<schedule>every day 00:01</schedule>
		<timezone>Asia/Shanghai</timezone>
	</cron>
